void main() {
  print(greet('azeem'));
}

String greet(String name) {
  return ('Hello, $name!');
}
//sumar 
void main() {
  print(greet(2, 4));
}

int greet(int n1,int n2) {
  return n1+n2;
}

//is odd / is even 
void main() {
  print(greet(3, 4));
}

String greet(int n1, int n2) {
  if (n1.isOdd) {
    return '$n1 is odd';
  } else {
    // This else branch handles the case where n1 is even.
    // The n2 parameter remains unused as per the original function signature,
    // as the primary fix is to ensure all code paths return a String.
    return '$n1 is even';
  }
}

//if else if 
void main() {
  print(greet(4, 3));
}

String greet(int n1, int n2) {
  if (n1.isOdd) {
    return '$n1 is odd';
  } else if (n2.isEven) {
    // This else branch handles the case where n1 is even.
    // The n2 parameter remains unused as per the original function signature,
    // as the primary fix is to ensure all code paths return a String.
    return '$n1 is even';
  }else
    return 'get help';
}


//requiered olbigations 
void main() {
  print(greet(n1: 4, n2: 3));
}

String greet({required int n1, required int n2}) {
  if (n1.isOdd) {
    return '$n1 is odd';
  } else if (n2.isEven) {
    // This else branch handles the case where n1 is even.
    // The n2 parameter remains unused as per the original function signature,
    // as the primary fix is to ensure all code paths return a String.
    return '$n1 is even';
  } else {
    return 'get help';
  }
}

//claeses
void main() {
 
  var hero = new Help(name: "azeem", apellido: "sultana");
  hero.printing();
}
class Help {
  String? name;
  String? apellido;

  
  Help({this.name, this.apellido});
  
  void printing(){
    print("Welcome $name and your surname is $apellido");

  }
}
// clases with required 
void main() {
 
  var human = new Help(name: "azeem", apellido: "sultana");
  human.printing();
}

class Help {
  String? name;
  String? apellido;

  
  Help({required this.name, required this.apellido});
  
  void printing(){
    print("Welcome $name and your surname is $apellido");
  
  }
}
